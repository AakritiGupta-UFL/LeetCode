class Solution {
    public List<String> fizzBuzz(int n) {
        List<String> numb=new ArrayList<String>();
        for(int i=1;i<n+1;i++)
        {
            if(i%3==0 && i%5==0)
            {
                numb.add("FizzBuzz");
            }
            else if(i%3==0)
            {numb.add("Fizz");}
            else if(i%5==0)
            {numb.add("Buzz");}
            else
                numb.add(Integer.toString(i));
                
        }
        return numb;
    }
}

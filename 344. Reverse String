class Solution {
    public String reverseString(String s) {
       int len=s.length();
       // int j=0;
        //char[]c=new char[len];
        StringBuilder c= new StringBuilder();
        for(int i=len-1;i>=0;i--)
        {
            c.append(s.charAt(i));
         //   j++;
        }
       String k= c.toString();
       return k;
    }
}
